// ( --- https://leetcode.com/problems/excel-sheet-column-number/ --- )



class Solution {
public:
    int titleToNumber(string columnTitle) {
        int number = 0;
        int n  = columnTitle.length();
        for(int i = n-1 ; i >= 0 ; i--){
            char no = columnTitle[n - i - 1]-64;
             number += pow(26,i)*(no);
        }
        return number;
    }
};
